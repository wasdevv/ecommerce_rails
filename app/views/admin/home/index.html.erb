<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
          <%= render 'layouts/shared/admin/sidebar' %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                  <%= render 'layouts/shared/admin/topbar' %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                  <%= render 'layouts/shared/admin/content' %>
            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
              <%= render 'layouts/shared/admin/footer' %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
      <%= render 'layouts/shared/admin/layoutmodal' %>
    <!-- End Logout Modal-->
</body>